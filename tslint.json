{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name": false,
    "ordered-imports": false,
    "max-line-length": {
      "options": [
        100
      ]
    },
    "no-console": false,
    "no-implicit-dependencies": false,
    "semicolon": [
      true,
      "always"
    ],
    "trailing-comma": [
      false
    ]
  }
}
